{
    "title": "Launch Apps",
    "version": "0.1.0",
    "maintainers": [
        "Saafo"
    ],
    "homepage": "https://github.com/Saafo/Kapslox",
    "import_url": "karabiner://karabiner/assets/complex_modifications/import?url=https://raw.githubusercontent.com/Saafo/Kapslox/main/LaunchApps.json",
    "rules": [
        {
            "description": "Launch Apps",
            "manipulators": [
                {
                    "description": "fn + 1 -> Browser",
                    "type": "basic",
                    "from": {
                        "key_code": "1",
                        "modifiers": {
                            "mandatory": [
                                "fn"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open -a 'Arc.app'"
                        }
                    ]
                },
                {
                    "description": "fn + 2 -> Xcode",
                    "type": "basic",
                    "from": {
                        "key_code": "2",
                        "modifiers": {
                            "mandatory": [
                                "fn"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open -a '/Applications/Xcode.app'"
                        }
                    ]
                },
                {
                    "description": "fn + 3 -> Simulator",
                    "type": "basic",
                    "from": {
                        "key_code": "3",
                        "modifiers": {
                            "mandatory": [
                                "fn"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open -a 'Simulator.app'"
                        }
                    ]
                },
                {
                    "description": "fn + ` -> Lark",
                    "type": "basic",
                    "from": {
                        "key_code": "grave_accent_and_tilde",
                        "modifiers": {
                            "mandatory": [
                                "fn"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open -a 'Lark.app'"
                        }
                    ]
                }
            ]
        }
    ]
}
